{
  "name": "spacingjs",
  "version": "1.1.0",
  "description": "Measure spacing between elements on webpage.",
  "main": "./dist/spacing.js",
  "module": "./dist/spacing.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "analyze": "webpack --mode production --env analyze=true",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md}'",
    "ext": "node extensions.config.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenlei/spacingjs.git"
  },
  "author": "Steven Lei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenlei/spacingjs/issues"
  },
  "homepage": "https://github.com/stevenlei/spacingjs#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/webpack-env": "^1.18.3",
    "babel-loader": "^9.1.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4"
  },
  "unpkg": "dist/spacing.min.js",
  "jsdelivr": "dist/spacing.min.js"
}
